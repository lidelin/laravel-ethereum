{
    "name": "lidelin/laravel-ethereum",
    "description": "ethereum development tools for laravel",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "lidelin",
            "email": "lidelin1023@qq.com"
        }
    ],
    "require": {
        "sc0vu/web3.php": "^0.1.4",
        "web3p/ethereum-tx": "^0.3.3"
    },
    "autoload": {
        "psr-4": {
            "LDL\\Ethereum\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LDL\\Ethereum\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LDL\\Ethereum\\ServiceProvider"
            ],
            "aliases": {
                "Web3": "LDL\\Ethereum\\Facade\\Web3"
            }
        }
    }
}
